@model IEnumerable<QLBanHang.Areas.QuanLy.Models.KhachHang>

@{
    ViewBag.Title = "Index";
    Layout = "~/Areas/QuanLy/Views/Shared/_LayoutAdmin.cshtml";
}

<p>
    @Html.ActionLink("Create New", "Create", "KhachHangs1")
</p>
<table id="table_id" class="table" border="1">
    <thead>
        <tr style="background-color:#fbe1e1">
            <th>
                Họ Tên
            </th>
            <th>
                Số Điện Thoại
            </th>
            <th>
                Địa Chỉ
            </th>
         
            <th>
                Username
            </th>
            <th>
                Password
            </th>
            
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Hoten)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Sdt)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DiaChi)
            </td>
          
            <td>
                @Html.DisplayFor(modelItem => item.tenDN)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.pass)
            </td>
        </tr>
}
    </tbody>
</table>
