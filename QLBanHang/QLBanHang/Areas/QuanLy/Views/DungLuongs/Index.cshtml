@model IEnumerable<QLBanHang.Areas.QuanLy.Models.DungLuong>

@{
    ViewBag.Title = "Index";
    Layout = "~/Areas/QuanLy/Views/Shared/_LayoutAdmin.cshtml";
}

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<div class="col-lg-6">
    <div class="table-responsive">
        <table class="table table-bordered table-hover">
            <tr style="background-color:#dbf8ff">
                <th>
                    ID Dung lượng :
                </th>
                <th>
                    Dung Lượng
                </th>
            </tr>

            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.Id)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.DungTich)
                    </td>

                </tr>
            }

        </table>
    </div>
</div>