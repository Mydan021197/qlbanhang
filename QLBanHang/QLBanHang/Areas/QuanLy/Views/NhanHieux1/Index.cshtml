@model IEnumerable<QLBanHang.Areas.QuanLy.Models.NhanHieu>

@{
    ViewBag.Title = "Index";
    Layout = "~/Areas/QuanLy/Views/Shared/_LayoutAdmin.cshtml";
}
<p>
    @Html.ActionLink("Create New", "Create")
</p>
<div class="col-lg-6">
    <div class="table-responsive">
        <table class="table table-bordered table-hover">
            <tr style="background-color:#dbf8ff">
                <th>
                    Nhãn Hiệu :
                </th>
                <th></th>
            </tr>

            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.TenHieu)
                    </td>
                    <td>
                        @Html.ActionLink("Edit", "Edit", new { id = item.Id })

                    </td>
                </tr>
            }

        </table>
    </div>
</div>