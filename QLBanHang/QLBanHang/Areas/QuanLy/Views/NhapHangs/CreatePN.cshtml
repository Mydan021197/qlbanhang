@model QLBanHang.Areas.QuanLy.Models.PhieuNhap

@{
    ViewBag.Title = "CreatePN";
    Layout = "~/Areas/QuanLy/Views/Shared/_LayoutAdmin.cshtml";
}

@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
    <div class="row">
        <div class="col-lg-6">
            <div class="form-group">
                <div class="form-group has-success">
                    <label class="control-label" for="inputSuccess">Nhà Phân Phối</label>
                    @Html.DropDownList("var1", null, htmlAttributes: new { @class = "form-control", @onchange = "myFunction()" })
                </div>
            </div>
            <div class="form-group">
                <div class="col-md-offset-2 col-md-10">
                    <input type="submit" value="Create" class="btn btn-default" />
                </div>
            </div>
        </div>
    </div>
}
