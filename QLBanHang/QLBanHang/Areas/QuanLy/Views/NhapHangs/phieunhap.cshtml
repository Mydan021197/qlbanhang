@model IEnumerable<QLBanHang.Areas.QuanLy.Models.PhieuNhap>

@{
    ViewBag.Title = "phieunhap";
    Layout = "~/Areas/QuanLy/Views/Shared/_LayoutAdmin.cshtml";
}

<p>
    @Html.ActionLink("Create New", "CreatePN","NhapHangs")
</p>
<div class="col-lg-12">
    <div class="panel panel-danger">
        <div class="panel-heading">
            Phiếu Nhập
        </div>
        <div class="panel-body">
            <table id="table_id" class="table" border="1" >
                <thead>
                <tr>
                    <th style="width:200px">
                        Nhà Phân Phối
                    </th>
                    <th style="width:200px">
                        Ngày Nhập
                    </th>
                    <th style="width:200px">
                        Tên Nhân Viên
                    </th>
                    <th style="width:300px"></th>
                </tr>
                    </thead>
                <tbody>
                @foreach (var item in Model)
                {
                    <tr>
                        <td>
                            @Html.DisplayFor(modelItem => item.NPP.TenNPP)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.NgayNhap)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.TenNV)
                        </td>
                        <td>
                            @Html.ActionLink("Chi Tiết Phiếu Nhập", "CTphieunhap", new { id = item.id})
                        </td>
                    </tr>
                }
            </tbody>
            </table>
            </div>
        </div>
    </div>
